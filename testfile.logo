println("creating turtle")
t = Turtle()
println("Moving Turtle")
t.fd(30)
t.set_angle(90)

fun draw_triangle(turtle, len){
    num=0
    turtle.fd(len/2)
    while(num<2)
    {
        turtle.rotate(120)
        turtle.fd(len)
        num = num + 1
    }
    turtle.rotate(120)
    turtle.fd(len/2)
}

draw_triangle(t,100)

