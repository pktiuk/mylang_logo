print("Moving Turtle")
t.move(30)
t.set_angle(90)

fun draw_triangle(turtle, len){
    num=0
    turtle.move(len/2)
    while(num<2)
    {
        turtle.rotate(120)
        turtle.move(len)
        num = num + 1
    }
    turtle.rotate(120)
    turtle.move(len/2)
}

draw_triangle(t,100)

